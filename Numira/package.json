{
  "name": "numira",
  "version": "1.0.0",
  "description": "AI-Powered Mental Clarity App",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:dev": "nodemon server.js --require dotenv/config",
    "start:prod": "node server.js",
    "client": "cd client && npm start",
    "dev:full": "concurrently \"npm run start:dev\" \"npm run client\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrate": "prisma migrate dev",
    "seed": "node scripts/seed.js",
    "setup:test": "node scripts/setup-test-env.js",
    "generate:vapid": "node scripts/generate-vapid-keys.js",
    "cap:init": "cd client && npx cap init Numira com.byteworthy.numira --web-dir=build",
    "cap:add:android": "cd client && npx cap add android",
    "cap:add:ios": "cd client && npx cap add ios",
    "cap:sync": "cd client && npx cap sync",
    "cap:open:android": "cd client && npx cap open android",
    "cap:open:ios": "cd client && npx cap open ios",
    "setup:cron": "node scripts/setup-cron-jobs.js",
    "verify:env": "node scripts/verify-env.js",
    "create:migration": "node scripts/create-prisma-migration.js",
    "create:schema": "node scripts/create-schema-migration.js",
    "create:auth": "node scripts/create-auth-migration.js",
    "create:webhook": "node scripts/create-webhook-migration.js",
    "create:emergency": "node scripts/create-emergency-migration.js",
    "create:platform": "node scripts/create-platform-migration.js",
    "create:family": "node scripts/create-family-plan-migration.js",
    "create:journal": "node scripts/create-journal-migration.js",
    "setup:monitoring": "node scripts/setup-failure-monitoring.js",
    "e2e:test": "node scripts/e2eTest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byteworthy/Numira.git"
  },
  "keywords": [
    "mental-health",
    "ai",
    "conversation",
    "clarity",
    "nodejs",
    "react",
    "postgresql"
  ],
  "author": "Byteworthy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/byteworthy/Numira/issues"
  },
  "homepage": "https://github.com/byteworthy/Numira#readme",
  "dependencies": {
    "@bull-board/api": "^5.0.0",
    "@bull-board/express": "^5.0.0",
    "@prisma/client": "^4.14.0",
    "anthropic": "^0.6.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.10.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "markdown-it": "^13.0.1",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.2",
    "openai": "^3.2.1",
    "pdfkit": "^0.13.0",
    "pg": "^8.11.0",
    "postmark": "^3.0.18",
    "prom-client": "^14.2.0",
    "redis": "^4.6.6",
    "stripe": "^12.5.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.0",
    "web-push": "^3.6.1",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.4",
    "concurrently": "^8.0.1",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "prisma": "^4.14.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/client/"
    ],
    "setupFilesAfterEnv": [
      "./tests/setup.js"
    ],
    "globalSetup": "./tests/global-setup.js",
    "globalTeardown": "./tests/global-teardown.js"
  }
}
